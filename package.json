{
  "name": "apollo-link-algolia",
  "version": "0.0.6",
  "description": "Query Algoila with Apollo",
  "main": "./lib/index.js",
  "repository": "git@github.com:janowsiany/apollo-link-algolia.git",
  "author": "Jan Owsiany <jan.owsiany@gmail.com>",
  "license": "MIT",
  "private": false,
  "keywords": [
    "algolia",
    "apollo",
    "apollo-link",
    "graphql"
  ],
  "scripts": {
    "dev": "babel src --out-dir lib --ignore '**/__tests__' --watch",
    "prepublish": "babel src --out-dir lib --ignore '**/__tests__'",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "dependencies": {
    "algoliasearch-helper": "^3.2.2",
    "apollo-link": "^1.2.14",
    "apollo-utilities": "^1.3.4",
    "graphql": "^15.3.0",
    "graphql-anywhere": "^4.2.7"
  },
  "peerDependencies": {
    "algoliasearch": "^4.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.2.13",
    "prettier": "^2.1.1"
  },
  "lint-staged": {
    "src/**/*.{js,json}": [
      "prettier --write --config ./.prettierrc",
      "git add"
    ]
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.js"
    ]
  }
}
